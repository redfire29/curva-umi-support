---
import Layout from "../layouts/Layout.astro";
import SonglistContent from "../components/pages/Songlist.svelte";
import ja from "../locales/ja.yml";

const locale = "ja";
const messages = ja;
const title = ja["song-list"] || "Song List";
---

<Layout title={title}>
  <SonglistContent client:load locale={locale} messages={messages} />
</Layout>
